{
    "name": "Div Highlighter",
    "version": "1.0",
    "description": "tool to highlight divs!",
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://*/",
        "https://*/"
    ],
    "manifest_version": 2
}
